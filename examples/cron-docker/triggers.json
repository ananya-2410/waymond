{
  "triggers": [
    {
      "type": "cron",
      "id": "global_cron",
      "config": {
        "expression": "*/1 * * * *"
      }
    },
    {
      "type": "buildkite",
      "id": "buildkite_ci_job_queues",
      "config": {
        "allow_queues": [
          "*"
        ],
        "reject_queues": [
          "dev-*"
        ]
      }
    }
  ]
}